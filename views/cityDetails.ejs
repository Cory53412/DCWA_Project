<html>

<body>
       <!--Make a border for the tables-->
    <style>
        table, th, td {
          border: 1px solid black;
          border-collapse: collapse;
        }
        </style>
           <!--Create a table to display the data-->
    <table>
        <tr>
            <th>City Code</th>
            <th>Country Code</th>
            <th>City Name</th>
            <th>population</th>
            <th>isCoastal</th>
            <th>areaKM</th>
               <!--access the data by calling cities from index.js and displaying its info-->
        </tr><% cities.forEach((cities)=>{%><tr>
            <td><%= cities.cty_code %></td>
            <td><%= cities.co_code %></td>
            <td><%= cities.cty_name %></td>
            <td><%= cities.population %></td>
            <td><%= cities.isCoastal %></td>
            <td><%= cities.areaKM %></td>
        </tr>
        <%})%>
    </table>
       <!--Button to link back to cities page-->
       <a href="http://localhost:3000/">Home</a>
      
</body>


</html>