<html>

<body>
    <style>
        table,
        th,
        td {
            border: 1px solid black;
            border-collapse: collapse;
        }
        </style> 

    <table>
        <tr>
            <th>ID</th>
            <th>headOfState</th>
        </tr>
    <tr><% headsOfState.forEach((headsOfState)=>{%><tr>
        <td><%= headsOfState._id %></td>
        <td><%= headsOfState.headsOfState %></td>
    </tr>
    <%})%>

    </table>
</body>

</html>